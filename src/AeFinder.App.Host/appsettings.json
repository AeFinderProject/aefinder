{
  "App": {
    "CorsOrigins": "https://*.AeFinder.com,http://localhost:4200,http://127.0.0.1:8081"
  },
  "ConnectionStrings": {
    "Default": "mongodb://localhost:27017/AeFinder"
  },
  "ChainNode": {
    "NodeConfigList": [
      {
        "ChainId": "AElf",
        "Endpoint": "http://127.0.0.1:8000"
      },
      {
        "ChainId": "tDVV",
        "Endpoint": "http://127.0.0.1:8001"
      }
    ]
  },
  "ElasticUris": {
    "Uris": [
      "http://127.0.0.1:9200"
    ]
  },
  "Kafka" : {
    "Brokers":["127.0.0.1:9092"],
    "MessageMaxBytes": 104857600
  },
  "IndexSetting": {
    "NumberOfShards": 5,
    "NumberOfReplicas": 1,
    "IndexPrefix": "AeFinder"
  },
  "Orleans": {
    "ClusterId": "dev",
    "ServiceId": "OrleansBasics",
    "MongoDBClient": "mongodb://localhost:27017",
    "DataBase": "AeFinderOrleansDB",
    "ClusterDbConnection": "127.0.0.1:6379",
    "ClusterDbNumber": 0
  },
  "AppInfo": {
    "AppId": "",
    "Version": ""
  },
  "RabbitMQ": {
    "Connections": {
      "Default": {
        "HostName": "localhost",
        "Port": "5672",
        "UserName": "guest",
        "Password": "guest"
      }
    },
    "EventBus": {
      "ClientName": "AeFinder_App-AppId-Version",
      "ExchangeName": "AeFinder_App-Version",
      "PrefetchCount": 20
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Default": "Warning",
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "Path": "Logs/log-.log",
          "retainedFileCountLimit": 15,
          "rollOnFileSizeLimit": true,
          "rollingInterval" : "Day",
          "formatter": {
            "type": "AeFinder.App.AeFinderJsonFormatter, AeFinder.Dapp"
          }
        }
      }
    ]
  }
}