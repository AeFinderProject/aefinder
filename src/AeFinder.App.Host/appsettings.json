{
  "App": {
    "CorsOrigins": "https://*.AeFinder.com,http://localhost:4200,http://127.0.0.1:8081"
  },
  "ConnectionStrings": {
    "Default": "mongodb://localhost:27017/AeFinder"
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*:8800/"
      }
    }
  },
  "ChainNode": {
    "ChainNodes": {
      "AELF":"http://127.0.0.1:8000",
      "tDVV":"http://127.0.0.1:8000"
    }
  },
  "Elasticsearch": {
    "Uris": [
      "http://127.0.0.1:9200"
    ],
    "NumberOfShards": 5,
    "NumberOfReplicas": 1
  },
  "AElfEntityMapping": {
    "CollectionPrefix": "AeFinder"
  },
  "Kafka" : {
    "Brokers":["127.0.0.1:9092"],
    "Partitions": 1,
    "ReplicationFactor": 1
  },
  "Orleans": {
    "ClusterId": "dev",
    "ServiceId": "OrleansBasics",
    "MongoDBClient": "mongodb://localhost:27017",
    "DataBase": "AeFinderOrleansDB"
  },
  "ApiHostUrl": "http://127.0.0.1:8081",
  "AppInfo": {
    "AppId": "appid",
    "Version": "version"
  },
  "AmazonS3": {
    "AccessKeyID": "",
    "SecretKey": "",
    "ServiceURL": "",
    "BucketName": ""
  },
  "ScheduledTask": {
    "AppRescanCheckTaskPeriodMilliSeconds": 600000
  },
  "RabbitMQ": {
    "Connections": {
      "Default": {
        "HostName": "localhost",
        "Port": "5672",
        "UserName": "guest",
        "Password": "guest"
      }
    },
    "EventBus": {
      "ClientName": "AeFinder_App-AppId-Version",
      "ExchangeName": "AeFinder_App-Version",
      "PrefetchCount": 20
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Default": "Warning",
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "Path": "Logs/log-.log",
          "retainedFileCountLimit": 15,
          "rollOnFileSizeLimit": true,
          "rollingInterval" : "Day",
          "formatter": {
            "type": "AeFinder.App.AeFinderJsonFormatter, AeFinder.App.Host"
          }
        }
      }
    ]
  },
  "OpenTelemetry": {
    "ServiceName": "AeFinder.App.Host",
    "ServiceVersion": "1.0.0",
    "CollectorEndpoint": "http://localhost:4315"
  }
}