<Project Sdk="Microsoft.NET.Sdk">

  <Import Project="..\..\common.props" />

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <RootNamespace>AElfIndexer</RootNamespace>
    <GenerateEmbeddedFilesManifest>true</GenerateEmbeddedFilesManifest>
    <PackageId>AElfIndexer.Domain.Shared</PackageId>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageVersion>1.1.0</PackageVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.FileProviders.Embedded" Version="7.0.1" />
    <PackageReference Include="Volo.Abp.Identity.Domain.Shared" Version="7.0.0-rc.5" />
    <PackageReference Include="Volo.Abp.OpenIddict.Domain.Shared" Version="7.0.0-rc.5" />
    <PackageReference Include="Volo.Abp.BackgroundJobs.Domain.Shared" Version="7.0.0-rc.5" />
    <PackageReference Include="Volo.Abp.AuditLogging.Domain.Shared" Version="7.0.0-rc.5" />
    <PackageReference Include="Volo.Abp.TenantManagement.Domain.Shared" Version="7.0.0-rc.5" />
    <PackageReference Include="Volo.Abp.FeatureManagement.Domain.Shared" Version="7.0.0-rc.5" />
    <PackageReference Include="Volo.Abp.PermissionManagement.Domain.Shared" Version="7.0.0-rc.5" />
    <PackageReference Include="Volo.Abp.SettingManagement.Domain.Shared" Version="7.0.0-rc.5" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Localization\AElfIndexer\*.json" />
    <Content Remove="Localization\AElfIndexer\*.json" />
  </ItemGroup>

</Project>
