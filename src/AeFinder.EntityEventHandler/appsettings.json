{
  "RabbitMQ": {
    "Connections": {
      "Default": {
        "HostName": "localhost",
        "Port": "5672",
        "UserName": "guest",
        "Password": "guest"
      }
    },
    "EventBus": {
      "ClientName": "AeFinder-ScanEntityHandler",
      "ExchangeName": "AeFinderExchange",
      "PrefetchCount": 100
    }
  },
  "ConnectionStrings": {
    "Default": "mongodb://localhost:27017/AeFinder"
  },
  "Redis": {
    "Configuration": "127.0.0.1"
  },
  "ElasticUris": {
    "Uris": [
      "http://127.0.0.1:9200"
    ]
  },
  "IndexSetting": {
    "NumberOfShards": 5,
    "NumberOfReplicas": 1,
    "IndexPrefix": "AeFinder"
  },
  "Orleans": {
    "ClusterId": "dev",
    "ServiceId": "OrleansBasics",
    "MongoDBClient": "mongodb://localhost:27017/?maxPoolSize=555",
    "DataBase": "AeFinderOrleansDB",
    "ClusterDbConnection": "127.0.0.1:6379",
    "ClusterDbNumber": 0
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Default": "Warning",
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "Path": "Logs/log-.log",
          "retainedFileCountLimit": 15,
          "rollOnFileSizeLimit": true,
          "rollingInterval" : "Day"
        }
      }
    ]
  }
}