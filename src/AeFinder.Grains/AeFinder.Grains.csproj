<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net7.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <PackageId>AeFinder.Grains</PackageId>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.Orleans.CodeGenerator.MSBuild" Version="3.6.5">
        <PrivateAssets>all</PrivateAssets>
        <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
      </PackageReference>
      <PackageReference Include="Microsoft.Orleans.Core.Abstractions" Version="3.6.5" />
      <PackageReference Include="Volo.Abp.Autofac" Version="7.0.0" />
      <PackageReference Include="Volo.Abp.BackgroundJobs.Domain" Version="7.0.0" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\AeFinder.Application.Contracts\AeFinder.Application.Contracts.csproj" />
        <ProjectReference Include="..\AeFinder.Kubernetes\AeFinder.Kubernetes.csproj" />
    </ItemGroup>

    <ItemGroup>
      <Reference Include="AeFinder.Kubernetes">
        <HintPath>..\AeFinder.Kubernetes\bin\Debug\net7.0\AeFinder.Kubernetes.dll</HintPath>
      </Reference>
    </ItemGroup>




</Project>
