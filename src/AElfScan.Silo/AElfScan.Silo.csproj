<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
      <ProjectReference Include="..\AElfScan.Application\AElfScan.Application.csproj" />
      <ProjectReference Include="..\AElfScan.EntityFrameworkCore\AElfScan.EntityFrameworkCore.csproj" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.Orleans.OrleansRuntime" Version="3.6.5" />
        <PackageReference Include="Microsoft.Orleans.Persistence.AdoNet" Version="3.6.5" />
        <PackageReference Include="Microsoft.Orleans.Runtime.Abstractions" Version="3.6.5" />
        <PackageReference Include="Serilog.AspNetCore" Version="5.0.0" />
        <PackageReference Include="Serilog.Sinks.Async" Version="1.5.0" />
        <PackageReference Include="Serilog.Sinks.RollingFile" Version="3.3.0" />
        <PackageReference Include="Microsoft.AspNetCore.DataProtection.StackExchangeRedis" Version="6.0.5" />
        <PackageReference Include="Volo.Abp.AspNetCore.Serilog" Version="6.0.0" />
        <PackageReference Include="Volo.Abp.Autofac" Version="6.0.0" />
        <PackageReference Include="Volo.Abp.Caching" Version="6.0.0" />
        <PackageReference Include="Volo.Abp.Caching.StackExchangeRedis" Version="6.0.0" />
        <PackageReference Include="Volo.Abp.Core" Version="6.0.0" />
    </ItemGroup>

    <ItemGroup>
        <None Remove="appsettings.json" />
        <Content Include="appsettings.json">
            <CopyToOutputDirectory>Always</CopyToOutputDirectory>
        </Content>
    </ItemGroup>

</Project>
