<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="EventStore.Client" Version="5.0.2" />
      <PackageReference Include="JsonNet.PrivateSettersContractResolvers" Version="1.0.0" />
      <PackageReference Include="Microsoft.Orleans.EventSourcing" Version="3.6.5" />
      <PackageReference Include="Microsoft.Orleans.Server" Version="3.6.5" />
      <PackageReference Include="Orleans.EventSourcing.Snapshot" Version="1.0.2" />
      <PackageReference Include="Orleans.Persistence.Redis" Version="3.2.2" />
      <PackageReference Include="Orleans.Providers.MongoDB" Version="3.8.0" />
      <PackageReference Include="Serilog.AspNetCore" Version="5.0.0" />
      <PackageReference Include="Serilog.Sinks.Async" Version="1.5.0" />
      <PackageReference Include="Volo.Abp.AspNetCore.Serilog" Version="5.3.4" />
      <PackageReference Include="Volo.Abp.Autofac" Version="5.3.4" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\AElfScan.Orleans.EventSourcing\AElfScan.Orleans.EventSourcing.csproj" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="appsettings.json" />
      <Content Include="appsettings.json">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </Content>
    </ItemGroup>

</Project>
